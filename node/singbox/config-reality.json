{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "v2ray_api": {
      "listen": "127.0.0.1:10085",
      "stats": {
        "enabled": true,
        "users": ["{{USERS}}"]
      }
    }
  },
  "inbounds": [
    {
      "type": "vless",
      "tag": "vless-reality",
      "listen": "::",
      "listen_port": {{LISTEN_PORT}},
      "sniff": true,
      "sniff_override_destination": true,
      "users": [
        {{USERS_JSON}}
      ],
      "tls": {
        "enabled": true,
        "server_name": "{{SNI}}",
        "reality": {
          "enabled": true,
          "handshake": {
            "server": "{{FALLBACK_ADDR}}",
            "server_port": {{FALLBACK_PORT}}
          },
          "private_key": "{{PRIVATE_KEY}}",
          "short_id": ["{{SHORT_ID}}"]
        }
      },
      "multiplex": {
        "enabled": true,
        "padding": true,
        "brutal": {
          "enabled": false
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "geoip": ["private"],
        "outbound": "block"
      }
    ],
    "final": "direct"
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8"
      },
      {
        "tag": "local",
        "address": "local"
      }
    ],
    "rules": [
      {
        "outbound": ["any"],
        "server": "local"
      }
    ],
    "final": "google"
  }
}
